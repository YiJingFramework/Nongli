<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		
		<AssemblyName>YiJingFramework.Nongli</AssemblyName>
		<RootNamespace>$(AssemblyName)</RootNamespace>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<IsTrimmable>true</IsTrimmable>

		<Version>5.0.1</Version>
		<PackageId>$(AssemblyName)</PackageId>
		<PackageProjectUrl>https://github.com/YiJingFramework/Nongli/wiki</PackageProjectUrl>
		<RepositoryUrl>https://github.com/YiJingFramework/Nongli.git</RepositoryUrl>
		<Description>此包提供了对中国农历的支持。 This package provides the support of the Chinese Nongli.</Description>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Authors>yueyinqiu</Authors>
		<Company>YiJingFramework</Company>
		<PackageIcon>Taichi.png</PackageIcon>
		<RepositoryType>git</RepositoryType>
		<PackageTags>YiJingFramework;农历;Nongli;Lunar;LunisolarCalendar;LunarCalendar;ChineseCalendar;ChineseLunisolarCalendar</PackageTags>
		<PackageReadmeFile>README.md</PackageReadmeFile>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\Taichi.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="..\..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="YiJingFramework.PrimitiveTypes.GanzhiCombinations" Version="2.0.1" />
	</ItemGroup>

</Project>
